{
  "name": "reactproject",
  "version": "1.0.0",
  "description": "A react project",
  "main": "webpack.config.js",
  "scripts": {
    "build:dev_server": "BABEL_ENVIRONMENT=dev BABEL_ENV=server babel src --out-dir ./dist/server",
    "build:dev_client": "webpack --env.ENVIRONMENT=dev --mode development --progress",
    "start:dev": "ENVIRONMENT=dev node ./dist/server/server_index.js",
    
    "build:prod_server": "BABEL_ENV=server babel src --out-dir ./dist/server",
    "build:prod_client": "webpack --mode production --progress",
    "start:prod": "node ./dist/server/server_index.js",
    
    "build:dev": "npm run build:dev_server && npm run build:dev_client && npm run start:dev",
    "build:prod": "npm run build:prod_server && npm run build:prod_client && openode deploy",
    
    "start": "npm run start:prod"
  },
